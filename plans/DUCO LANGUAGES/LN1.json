{
    "code": "LN1",
    "hetznerPackage": null,
    "locations": ["us3", "us2", "us1"],
    "specs": {
        "cpu": 100,
        "ram": 512,
        "disk": 2048
    },
    "isDedicatedCpu": false,
    "price": 5,
    "options": {
        "categories": ["ln1"],
        "userLimit": -1,
        "name": "Language 1"
    },
    "container": [{
        "runtime": "python",
        "allocationPort": 25565,
        "egg": 17,
        "docker_image": "ghcr.io/parkervcp/yolks:python_3.10",
        "startup": "if [[ -d .git ]] && [[ {{AUTO_UPDATE}} == '1' ]]; then git pull; fi; if [[ ! -z {{PY_PACKAGES}} ]]; then pip install -U --prefix .local {{PY_PACKAGES}}; fi; if [[ -f /home/container/${REQUIREMENTS_FILE} ]]; then pip install -U --prefix .local -r ${REQUIREMENTS_FILE}; fi; /usr/local/bin/python /home/container/{{PY_FILE}}",
        "environment": {
            "GIT_ADDRESS": "",
            "BRANCH": "",
            "USER_UPLOAD": "0",
            "AUTO_UPDATE": "0",
            "PY_FILE": "app.py",
            "PY_PACKAGES": "",
            "USERNAME": "",
            "ACCESS_TOKEN": "",
            "REQUIREMENTS_FILE": "requirements.txt"
        },
        "feature_limits": {
            "databases": 0,
            "backups": 1
        }
    }, {
        "runtime": "nodejs",
        "allocationPort": 25565,
        "egg": 16,
        "docker_image": "ghcr.io/parkervcp/yolks:nodejs_18",
        "startup": "if [[ -d .git ]] && [[ {{AUTO_UPDATE}} == '1' ]]; then git pull; fi; if [[ ! -z ${NODE_PACKAGES} ]]; then /usr/local/bin/npm install ${NODE_PACKAGES}; fi; if [[ ! -z ${UNNODE_PACKAGES} ]]; then /usr/local/bin/npm uninstall ${UNNODE_PACKAGES}; fi; if [ -f /home/container/package.json ]; then /usr/local/bin/npm install; fi; /usr/local/bin/node /home/container/{{JS_FILE}}",
        "environment": {
            "GIT_ADDRESS": "",
            "BRANCH": "",
            "USER_UPLOAD": "0",
            "AUTO_UPDATE": "0",
            "JS_FILE": "index.js",
            "NODE_PACKAGES": "",
            "USERNAME": "",
            "ACCESS_TOKEN": "",
            "UNNODE_PACKAGES": ""
        },
        "feature_limits": {
            "databases": 0,
            "backups": 1
        }
    }, {
        "runtime": "dart",
        "allocationPort": 25565,
        "egg": 19,
        "docker_image": "ghcr.io/parkervcp/yolks:dart_2.17",
        "startup": "if [[ -d .git ]] && [[ {{AUTO_UPDATE}} == '1' ]]; then git pull; fi; dart pub get; dart run",
        "environment": {
            "GIT_ADDRESS": "",
            "BRANCH": "",
            "USER_UPLOAD": "0",
            "AUTO_UPDATE": "0",
            "USERNAME": "",
            "ACCESS_TOKEN": ""
        },
        "feature_limits": {
            "databases": 0,
            "backups": 1
        }    
    }, {
        "runtime": "java",
        "allocationPort": 25565,
        "egg": 18,
        "docker_image": "ghcr.io/parkervcp/yolks:java_19",
        "startup": "java -Dterminal.jline=false -Dterminal.ansi=true -jar {{JARFILE}}",
        "environment": {
            "JARFILE": "sneakyhub.jar"
        },
        "feature_limits": {
            "databases": 0,
            "backups": 1
        }   
    }]
}